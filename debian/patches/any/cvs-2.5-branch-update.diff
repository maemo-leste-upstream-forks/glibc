diff -Nurd glibc-2.5.orig/stdlib/tst-atof1.c glibc-2.5.orig/stdlib/tst-atof1.c
--- glibc-2.5.orig/stdlib/tst-atof1.c	1970-01-01 01:00:00.000000000 +0100
+++ glibc-2.5.orig/stdlib/tst-atof1.c	2007-01-12 19:07:19.000000000 +0100
@@ -0,0 +1,19 @@
+#include <stdio.h>
+#include <stdlib.h>
+#include <string.h>
+
+static int
+do_test (void)
+{
+  char buf[100];
+  snprintf (buf, sizeof (buf), "%g", atof ("0x10p-1"));
+  if (strcmp (buf, "8") != 0)
+    {
+      printf ("got \"%s\", expected \"8\"\n", buf);
