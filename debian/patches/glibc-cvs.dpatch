+#define VSYSCALL_ADDR_vgettimeofday	0xffffffffff600000
+
+
+ENTRY (__gettimeofday)
+	/* Align stack.  */
+	sub	$0x8, %rsp
+	movq	$VSYSCALL_ADDR_vgettimeofday, %rax
+	callq	*%rax
+	/* Check error return.  */
+	cmpl	$-4095, %eax
+	jae	SYSCALL_ERROR_LABEL
+
+L(pseudo_end):
+	add	$0x8, %rsp
+	ret
+PSEUDO_END(__gettimeofday)
